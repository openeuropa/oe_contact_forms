{
    "name": "openeuropa/oe_contact_forms",
    "description": "OpenEuropa Contact Forms module.",
    "type": "drupal-module",
    "license": "EUPL-1.2",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0",
        "cweagans/composer-patches": "^1.4",
        "drupal/contact_storage": "^1.3",
        "drupal/contact_storage_export": "^1.15",
        "drupal/core": "^9.4 || ^10",
        "drupal/multivalue_form_element": "^1.0.0-beta6",
        "openeuropa/oe_corporate_countries": "^2.0.0-alpha8",
        "openeuropa/rdf_skos": "^1.0.0-alpha10"
    },
    "require-dev": {
        "composer/installers": "^1.11",
        "drupal/core-composer-scaffold": "^9.4 || ^10",
        "drupal/core-dev": "^9.4 || ^10",
        "drupal/honeypot": "^2.1",
        "drush/drush": "^11.1 || ^12",
        "openeuropa/code-review": "^2.0.0-alpha6",
        "openeuropa/task-runner-drupal-project-symlink": "^1.0-beta6",
        "phpspec/prophecy-phpunit": "^2"
    },
    "scripts": {
        "post-install-cmd": "./vendor/bin/run drupal:site-setup",
        "post-update-cmd": "./vendor/bin/run drupal:site-setup"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "autoload": {
        "psr-4": {
            "Drupal\\oe_contact_forms\\": "./src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Drupal\\Tests\\oe_contact_forms\\": "./tests/src/"
        }
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "enable-patching": true,
        "installer-paths": {
            "build/core": ["type:drupal-core"],
            "build/profiles/contrib/{$name}": ["type:drupal-profile"],
            "build/modules/contrib/{$name}": ["type:drupal-module"],
            "build/themes/contrib/{$name}": ["type:drupal-theme"]
        },
        "patches": {
            "drupal/contact_storage": {
                "https://www.drupal.org/project/contact_storage/issues/2708809": "https://www.drupal.org/files/issues/2022-11-07/revisit_permissions-2708809-60.patch",
                "https://www.drupal.org/project/contact_storage/issues/3053497": "https://www.drupal.org/files/issues/2020-09-28/3053497-8.patch"
            },
            "drupal/contact_storage_export": {
                "https://www.drupal.org/project/contact_storage_export/issues/2996037": "https://www.drupal.org/files/issues/2023-03-10/contact_storage_export-breaks-on-high-load-balancer-2996037-20.patch"
            }
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "./build"
            }
        },
        "drush": {
            "services": {
                "drush.services.yml": "^11"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "drupal/core-composer-scaffold": true,
            "phpro/grumphp": true,
            "phpstan/extension-installer": true
        }
    }
}
